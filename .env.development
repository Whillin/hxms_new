# 【开发】环境变量

# 网站地址前缀
VITE_BASE_URL = /

# API 请求基础路径（开发环境通常为代理前缀，如 /api ）
VITE_API_URL = /api

# 本地开发代理的目标后端地址（仅开发环境生效，用于解决跨域）
VITE_API_PROXY_URL = http://localhost:3001

# 是否启用本地 mock 插件（true=启用；false=禁用并走后端代理）
VITE_USE_MOCK = false

# 前端开发服务器端口（覆盖通用配置）
VITE_PORT = 3008

# Delete console
VITE_DROP_CONSOLE = false